# Appwrite Configuration
# Copy this file to .env and replace with your actual values

# Appwrite Cloud URL (or your self-hosted instance)
VITE_APPWRITE_URL=https://cloud.appwrite.io/v1

# Your Appwrite Project ID
VITE_APPWRITE_PROJECT_ID=your_project_id_here

# Database ID
VITE_APPWRITE_DATABASE_ID=your_database_id_here

# Collection ID for blog posts
VITE_APPWRITE_COLLECTION_ID=your_collection_id_here

# Storage Bucket ID for file uploads
VITE_APPWRITE_BUCKET_ID=your_bucket_id_here

# TinyMCE API Key for rich text editor
VITE_TINYMCE_API_KEY=your_tinymce_api_key_here
